<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.




    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.




        -->
    </style>
    <!-- Application theme. -->
    <style name="AppTheme" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <!-- All customizations that are NOT specific to a particular APId-level can go here. -->
    </style>

    <style name="CustomActionBarTheme2" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <!--<item name="android:popupMenuStyle">@style/PopupMenu.Customtheme</item>
        <item name="android:windowContentOverlay">@null</item>-->
    </style>

    <style name="PopupMenu.Customtheme" parent="@android:style/Widget.Holo.Light.ListPopupWindow">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_example</item>
    </style>

    <style name="mTheme" parent="Theme.AppCompat.Light.DarkActionBar"></style>

</resources>